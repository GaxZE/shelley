{
  "name": "@actionishope/shelley",
  "version": "1.0.0-alpha.1",
  "license": "ISC",
  "description": "React UI library, another one. This one uses 'stylable' from Wix for full 'themeability'.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/action-is-hope/shelley.git"
  },
  "keywords": [
    "a11y",
    "stylable",
    "react",
    "typescript"
  ],
  "author": "action-is-hope<info@action-is-hope.earth>",
  "bugs": {
    "url": "https://github.com/action-is-hope/shelley/issues"
  },
  "homepage": "https://github.com/action-is-hope/shelley#readme",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "copy": "copyfiles -u 1 \"./src/**/*.st.css\" \"./src/**/*.css\" \"./src/**/*.js\" dist && copyfiles package.json \"./package.json\" dist && copyfiles README.md \"./README.md\" dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production --no-devtool",
    "build-dist": "npm run clean && npm run copy && tsc --project tsconfig-npm.json",
    "start": "webpack serve --open",
    "serve": "serve ./dist",
    "lint": "eslint . -f codeframe",
    "typecheck": "tsc --project tsconfig-npm.json",
    "test": "npm run typecheck && npm run lint && jest"
  },
  "jest": {
    "verbose": true
  },
  "dependencies": {
    "@reach/menu-button": "^0.12.1",
    "@reach/popover": "0.12.1",
    "@stylable/cli": "^4.5.0",
    "classnames": "^2.2.6",
    "copyfiles": "^2.4.1",
    "downshift": "^6.1.3",
    "react-expanding-textarea": "^2.3.1",
    "react-popper": "^2.2.5"
  },
  "devDependencies": {
    "@stylable/core": "^4.3.0",
    "@stylable/jest": "^4.3.0",
    "@stylable/runtime": "^4.3.0",
    "@stylable/webpack-plugin": "^4.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-stylable": "^4.3.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.4.2",
    "match-sorter": "^6.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.0.9",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.4.3",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "serve": "^12.0.0",
    "ts-jest": "26.5.6",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.3",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
